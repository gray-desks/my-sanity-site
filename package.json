    "@sanity/vision": "^4.3.0",
    "dotenv": "^17.2.1",
    "gray-matter": "^4.0.3",
    "react": "^19.1",
    "react-dom": "^19.1",
    "react-is": "^19.1.1",
    "rehype-stringify": "^10.0.1",
    "remark": "^15.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "sanity": "^4.3.0",
    "styled-components": "^6.1.18"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@types/react": "^19.1",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
